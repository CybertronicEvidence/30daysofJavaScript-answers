1. let firstName = 'Evidence';
   let lastName = 'Adejuwon';
   let country = 'Nigeria';
   let city = 'Abuja';
   let age = 19;
   let isMarried = false;
   let year = 2022;
   console.log(typeof firstName, typeof lastName, typeof country, typeof city, typeof age, typeof isMarried, typeof year);
   // string, string, string, string, number, boolean, number
2. console.log(typeof '10' == typeof 10); // true
3. console.log(typeof parseInt('9.8') == typeof 10); // true
