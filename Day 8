1. const personWithTheHighestSkills = () => {
    const getHighestNumber = () => {
        const array = []
        array.push(Object.values(users.Alex.skills).length)
        array.push(Object.values(users.Asab.skills).length)
        array.push(Object.values(users.Brook.skills).length)
        array.push(Object.values(users.Daniel.skills).length)
        array.push(Object.values(users.John.skills).length)
        array.push(Object.values(users.Thomas.skills).length)
        array.push(Object.values(users.Paul.skills).length)
    
        const max = Math.max(...array);
        const index = array.indexOf(max);
    
        return index;
    }
    console.log(getHighestNumber())
    const mainKey = Object.keys(users)[getHighestNumber()]

    return mainKey
    }
    console.log(personWithTheHighestSkills())

2. const countUsersPoints = () => {
    const arr = []
    if (users.Alex.points >= 50 ) {
        arr.push(users.Alex.points)
    }
    if (users.Asab.points >= 50 ) {
        arr.push(users.Asab.points)
    }
    if (users.Brook.points >= 50 ) {
        arr.push(users.Brook.points)
    }
    if (users.Daniel.points >= 50 ) {
        arr.push(users.Daniel.points)
    }
    if (users.John.points >= 50 ) {
        arr.push(users.John.points)
    }
    if (users.Thomas.points >= 50 ) {
        arr.push(users.Thomas.points)
    }
    if (users.Paul.points >= 50 ) {
        arr.push(users.Paul.points)
    }

    const count = arr.length;

    return count;
    }
    console.log(countUsersPoints())

    OR

   const countPoints = () => {
    let count = 0;
    for (const key in users){
        let point = users[key].points
        console.log(point)
        if (point >= 50) {
            count++
        }
    }
    return count;
    }
    console.log(countPoints())

